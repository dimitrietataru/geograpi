{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Web.HTTP": {
      "commandName": "Project",
      "_launchBrowser": true,
      "_launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:43000"
    },
    "Web.HTTPS": {
      "commandName": "Project",
      "_launchBrowser": true,
      "_launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:42000;http://localhost:43000"
    },
    "Docker": {
      "commandName": "Docker",
      "_launchBrowser": true,
      "_launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:443;http://+:80"
      },
      "publishAllPorts": true,
      "useSSL": true
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  },
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:43000",
      "sslPort": 42000
    }
  }
}
